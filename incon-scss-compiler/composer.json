{
    "name": "incon/scss-compiler",
    "description": "Advanced SCSS compiler plugin for WordPress",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=7.2",
        "scssphp/scssphp": "^1.11"
    },
    "autoload": {
        "classmap": ["includes/"]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}